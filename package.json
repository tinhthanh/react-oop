{
  "name": "Vg-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "generate-pwa-assets": "pwa-assets-generator --preset minimal public/logo.svg",
    "start": "npm run lint && npm run dev",
    "dev": "tsc && vite --port 1301 --mode localdev",
    "build": "npm run build:prod",
    "build:dev": "npm run lint && tsc && vite build --mode development",
    "build:dev-local": "npm run lint && tsc && vite build --mode localdev",
    "build:prod": "npm run lint && tsc && vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 5",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "@ckeditor/ckeditor5-build-classic": "^42.0.0",
    "@ckeditor/ckeditor5-react": "^8.0.0",
    "antd": "^5.19.0",
    "axios": "^1.7.2",
    "boring-avatars": "^1.10.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.11",
    "dexie": "^4.0.8",
    "express": "^4.19.1",
    "firebase": "^10.12.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.5",
    "react-router-dom": "^6.26.0",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "tsyringe": "^4.8.0",
    "uuid": "^10.0.0",
    "validator": "^13.11.0",
    "vg-web-sdk": "^1.1.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.11.8",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "postcss": "^8.4.38",
    "sass": "^1.72.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-pwa": "^0.20.1"
  }
}
